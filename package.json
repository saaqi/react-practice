{
  "name": "saaqi-react-basics-course",
  "description": "My project to learn and follow along the React Basics course.",
  "private": true,
  "version": "0.0.1",
  "homepage": "https://saqibtech.com/react-practice",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --emptyOutDir",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
